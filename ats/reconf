#!/bin/sh
echo removing junk...
rm -f aclocal.m4 config.cache config.h configure *.in src/*.in config.status Makefile src/Makefile
rm -rf autom4te.cache/
echo running aclocal...
aclocal
echo running autoconf...
autoconf
echo running autoheader...
autoheader
echo running automake...
automake --foreign -a
